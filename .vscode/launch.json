{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "request": "launch",
            "name": "Run PrisonersDilemmaSimulation",
            "mainClass": "PrisonersDilemmaSimulation",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "classPaths": [
                "${workspaceFolder}/bin",
                "${workspaceFolder}/lib/*"
            ],
            "vmArgs": "--class-path ${workspaceFolder}/bin;${workspaceFolder}/lib/*"
        }
    ]
}
